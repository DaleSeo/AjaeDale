---
export interface Props {
  size?: number;
  class?: string;
  id?: string;
}

const { size = 16, class: className = "", id } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={size}
  height={size}
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
  id={id}
>
  <path d="M18 6 6 18"></path>
  <path d="m6 6 12 12"></path>
</svg>
